{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "---",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "targets": {
    "global": {
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "alias": {
    "shared": "./src/shared"
  },
  "scripts": {
    "dev": "npm run checkTsc && parcel static/pages/*.html",
    "start": "npm run build && node server.js",
    "build": "npm run checkTsc && npm run linter && parcel build static/pages/*.html",
    "checkTsc": "npx tsc --noEmit",
    "linter": "npx eslint . --ext .ts --fix",
    "slinter": "npx stylelint '**/*.scss' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habirov91/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
