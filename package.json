  },
  "homepage": "https://github.com/habirov91/middle.messenger.praktikum.yandex#readme",
  "dependencies": {
    "express": "4.17.2",
    "handlebars": "4.7.7",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
